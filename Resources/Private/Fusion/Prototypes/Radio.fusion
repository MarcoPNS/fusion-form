prototype(Neos.Fusion.Form:Radio)  < prototype(Neos.Fusion.Form:FieldComponent) {

    renderer = afx`
        <input
            value={Form.stringifyValue(props.value)}
            checked={null}
            checked.@process.singleValue={Form.stringifyValue(field.value) == Form.stringifyValue(props.value)}
            checked.@process.singleValue.@if.has={field.value}
            {...props}
        />
    `

}
