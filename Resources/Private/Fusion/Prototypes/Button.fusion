prototype(Neos.Fusion.Form:Button) < prototype(Neos.Fusion.Form:Field) {
    renderer = afx`
        <button
            {...props.attributes}
            id={props.id}
            class={props.class}
            type="submit"
            name={field.name}
            value={Form.stringifyValue(field.value)}
            />
    `
}
