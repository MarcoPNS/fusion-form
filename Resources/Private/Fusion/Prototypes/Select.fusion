prototype(Neos.Fusion.Form:Select)  < prototype(Neos.Fusion.Form:FieldComponent) {
    @propTypes {
        content = ${PropTypes.string}
    }

    content = null

    renderer = afx`
        <select
            id={props.id}
            class={props.class}
            name={field.name}
            required={props.required}
            multiple={field.multiple}
            {...props.attributes}
        >
            {props.content}
        </select>
    `


}
